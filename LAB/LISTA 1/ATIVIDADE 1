	printf ("Digite a segunda nota");
	scanf ("% f", & nota2);
	media = (nota1 + nota2) / 2;
	if (media> = 7 && media <10) {
		printf ("Aprovado");
	}
	outro
	if (mídia <7) {
		printf ("Reprovado");
	} outro {

	printf ("Aprovado com Distincao"); }
	sistema ("pausa");
	retornar 0;
	
} 