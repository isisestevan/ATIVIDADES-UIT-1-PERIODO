#include <stdio.h>
#include <stdlib.h>
int main () {
	flutuar a, b, c;
	printf ("\ nInforme o primeiro lado");
	scanf ("% f", & a);
	printf ("\ nInforme o segundo lado");
	scanf ("% f", & b);
	printf ("\ nInforme o terceiro lado");
	scanf ("% f", & c);
	if (a <b + c && b <a + c && c <a + b) {
		if (a == b && b == c) {
			printf ("\ nTriangulo Equilatero");
		}outro 
		if (a == b || b == c) {
			printf ("\ nTroculo isoceles");
		}outro
		printf ("\ nTriangulo Escaleno");
	}
	outro
	printf ("\ nLados não conseguem um triângulo");
	sistema ("pausa");
	retornar 0;
}
 